{
  "name": "fireblocks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "dotenv -e .env.development.local next build",
    "postinstall": "npm run generate",
    "start": "next start",
    "lint": "next lint",
    "generate": "npx prisma generate",
    "db:push": "dotenv -e .env.development.local -- npx prisma db push --accept-data-loss",
    "db:studio": "dotenv -e .env.development.local -- npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@prisma/extension-accelerate": "^0.6.2",
    "fireblocks-sdk": "^5.12.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5"
  }
}
